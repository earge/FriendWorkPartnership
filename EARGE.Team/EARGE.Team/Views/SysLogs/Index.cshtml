@model IEnumerable<EARGE.DataLayer.SysLog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TargetObjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SysLogType.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeamMember.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TargetObjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SysLogType.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamMember.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
